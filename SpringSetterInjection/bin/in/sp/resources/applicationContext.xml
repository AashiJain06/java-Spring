<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

     <bean class="in.aj.beans.Address" id="addId">
       <property name="city" value="Indore"></property>
       <property name="pin" value="452003"></property>
       <property name="house" value="64"></property>
     </bean>

   <bean class="in.aj.beans.Student" id="studId">
   <property name="name" value="Aashi"></property>
   <property name="roll" value="101"></property>
   <property name="addres" ref="addId"></property>
   </bean>

</beans>
